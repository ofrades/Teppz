{
  "name": "eppz! Code (json)",
  "settings": [
    {
      "name": "Json (array punctuation)",
      "scope": [
        "punctuation.definition.array.begin",
        "punctuation.definition.array.end",
        "punctuation.separator.array"
      ],
      "settings": {
        "foreground": "#8ba9ac"
      }
    },
    {
      "name": "Json (dictionary punctuation)",
      "scope": [
                 "punctuation.definition.dictionary.begin",
                 "punctuation.definition.dictionary.end",
                 "punctuation.separator.dictionary",
                 "meta.structure.dictionary.json",
                 "meta.structure.dictionary.value.json"
               ],
      "settings": {
        "foreground": "#6a6656"
      }
    },
    {
      "name": "Json (property)",
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#b6776b",
        "fontStyle": ""
      }
    },
    {
      "name": "Json (property punctuation)",
      "scope": ["support.type.property-name punctuation"],
      "settings": {
        "foreground": "#6a6656"
      }
    },
    {
      "name": "Json (string)",
      "scope": ["string - support.type.property-name"],
      "settings": {
        "foreground": "#60c0d2"
      }
    },
    {
      "name": "Json (string punctuation)",
      "scope": [
        "punctuation.definition.string.begin.json",
        "punctuation.definition.string.end.json"
      ],
      "settings": {
        "foreground": "#4a6269"
      }
    },
    {
      "scope": "invalid.illegal.character-not-allowed-here.html",
      "settings": {
        "foreground": "#CF7357"
      }
    }
  ]
}
