{
  "name": "Teppz Light",
  "type": "light",
  "include": "eppz-code-light-root.json",
  "colors": {
    "editor.background": "#ccc9b8",
    "tab.activeBackground": "#ccc9b8",
    "editorGutter.background": "#ccc9b8",
    "list.inactiveSelectionBackground": "#ccc9b8",
    "list.activeSelectionBackground": "#ccc9b8",
    "list.focusBackground": "#ccc9b8",
    "menu.selectionBackground": "#ccc9b8",
    "input.background": "#ccc9b8",
    "list.hoverBackground": "#fff5",
    "editorIndentGuide.activeBackground": "#aaa",
    "editorIndentGuide.background": "#d2d2d2",
    "menu.separatorBackground": "#bbb",
    "panel.border": "#fff0",
    "sideBar.border": "#bbb",
    "sideBarSectionHeader.border": "#bbb",
    "tab.border": "#bbb",
    "input.border": "#bbb",
    "sideBar.foreground": "#555",
    "dropdown.background": "#fff5",
    "notificationToast.border": "#bbb",
    "notifications.border": "#bbb",
    "widget.shadow": "#fff0",
    "scrollbar.shadow": "#fff0",
    "sideBarSectionHeader.background": "#fff0",
    "statusBar.border": "#bbb",
    "statusBar.background": "#e6e2cf",
    "menu.background": "#e6e2cf",
    "editor.lineHighlightBackground": "#e6e2cf",
    "settings.dropdownBackground": "#e6e2cf",
    "inputValidation.errorBackground": "#e6e2cf",
    "inputValidation.infoBackground": "#e6e2cf",
    "inputValidation.warningBackground": "#e6e2cf",
    "list.dropBackground": "#e6e2cf",
    "activityBar.background": "#e6e2cf",
    "sideBar.background": "#e6e2cf",
    "editorPane.background": "#e6e2cf",
    "editorGroupHeader.noTabsBackground": "#e6e2cf",
    "editorGroupHeader.tabsBackground": "#e6e2cf",
    "tab.inactiveBackground": "#e6e2cf",
    "editorWidget.background": "#e6e2cf",
    "editorWidget.border": "#e6e2cf",
    "editorSuggestWidget.background": "#e6e2cf",
    "editorSuggestWidget.border": "#e6e2cf",
    "editorHoverWidget.background": "#e6e2cf",
    "editorHoverWidget.border": "#e6e2cf",
    "editorMarkerNavigation.background": "#e6e2cf",
    "editorMarkerNavigationError.background": "#e6e2cf",
    "editorMarkerNavigationWarning.background": "#e6e2cf",
    "editorMarkerNavigationInfo.background": "#e6e2cf",
    "peekViewEditor.background": "#e6e2cf",
    "peekViewEditorGutter.background": "#e6e2cf",
    "peekViewResult.background": "#e6e2cf",
    "panel.background": "#e6e2cf",
    "titleBar.activeBackground": "#e6e2cf",
    "titleBar.inactiveBackground": "#e6e2cf",
    "terminal.background": "#e6e2cf",
    "walkThrough.embeddedEditorBackground": "#e6e2cf",
    "editorCursor.foreground": "#62bb47",
    "activityBarBadge.background": "#388E3C",
    "scrollbarSlider.activeBackground": "#388E3C50",
    "editorSuggestWidget.highlightForeground": "#388E3C",
    "textLink.foreground": "#388E3C",
    "progressBar.background": "#388E3C",
    "pickerGroup.foreground": "#388E3C",
    "notificationLink.foreground": "#388E3C",
    "settings.modifiedItemIndicator": "#388E3C",
    "settings.headerForeground": "#388E3C",
    "breadcrumb.activeSelectionForeground": "#388E3C",
    "menu.selectionForeground": "#388E3C",
    "menubar.selectionForeground": "#388E3C",
    "selection.background": "#388E3C40",
    "activityBar.foreground": "#388E3C",
    "activityBar.inactiveForeground": "#555",
    "foreground": "#555",
    "scrollbarSlider.background": "#5555",
    "editor.foreground": "#555",
    "statusBar.foreground": "#555"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#272B35",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#3091a2"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "foreground": "#a99649"
      }
    },
    {
      "name": "Keyword",
      "scope": ["punctuation.accessor", "keyword"],
      "settings": {
        "foreground": "#9E735F",
        "fontStyle": "italic"
      }
    },
    {
      "name": "HTML Tag names",
      "scope": [
        "entity.name.tag",
        "meta.tag.other.html",
        "meta.tag.other.js",
        "meta.tag.other.tsx",
        "entity.name.tag.tsx",
        "entity.name.tag.js",
        "entity.name.tag",
        "meta.tag.js",
        "meta.tag.tsx",
        "meta.tag.html"
      ],
      "settings": {
        "foreground": "#5D5050",
        "fontStyle": ""
      }
    },
    {
      "name": "ID Attribute Name in HTML",
      "scope": "entity.other.attribute-name.id.html",
      "settings": {
        "foreground": "#304391"
      }
    },
    {
      "name": "HTML Punctuation Definition Tag",
      "scope": "punctuation.definition.tag.html",
      "settings": {
        "foreground": "#1E9248"
      }
    },
    {
      "name": "HTML Doctype",
      "scope": "meta.tag.sgml.doctype.html",
      "settings": {
        "foreground": "#9A7A4E",
        "fontStyle": "italic"
      }
    }
  ]
}
